<h1 class="row"><%= Spree.t(:wishlists) %></h1>
<table id="product-properties" class="table table-striped" data-hook>
<thead>
  <tr>
    <th>Name</th>
    <th>Date Created</th>
  </tr>
</thead>
<tbody>
    <% @wishlists.each do |wl| %>
        <% # if !wl.is_private? || @user == spree_current_user %>
          <% css_class = cycle('even', 'odd', :name => "wish") %>
          <tr class="<%= css_class %>">
            <td><%= link_to wl.name, wl %></td>
            <td><%= wl.created_at.strftime("%m/%d/%Y") %></td>
          </tr>
        <% # end %>
    <% end %>
  <% reset_cycle('wish') %>
</tbody>
</table>
<%= link_to "Create Wishlist", new_wishlist_path, class: "btn btn-primary" %>
